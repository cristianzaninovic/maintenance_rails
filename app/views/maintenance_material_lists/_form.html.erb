<%= simple_form_for @maintenance_material_list do |form| %>
  <%= form.error_notification %>
  <%= form.error_notification message: form.object.errors[:base].to_sentence if form.object.errors[:base].present? %>
  <div class="form-inputs">
    <%= form.association :material %>
    <%= form.association :maintenance %>
  </div>
  <div class="form-actions">
    <%= form.button :submit, class: 'btn btn-outline-primary ' %>  
  </div>  
<% end %>
